#lang R5RS
(#%provide (all-defined))

(#%require (planet williams/describe/describe))
(define get-type
  (lambda (x)
    (cond ((number? x) "Number")
          ((pair? x) "Pair")
          ((string? x) "String")
          ;((port? x) "Port")
          ((vector? x) "Vector")
          ((boolean? x) "Boolean")
          ((symbol? x) "Symbol")          
		  ((procedure? x) "Procedure")          
		  ))) 

(define (out.ln ln)
  (display ln)
  (display "\n"))